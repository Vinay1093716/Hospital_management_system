<div class="text-center">
<h1>New Doctor</h1>
</div>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @doctor,class:"form" do |f| %>
      <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name,class:"form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :speciality %>
        <%= f.text_field :speciality,class:"form-control" %>
      </div>
        <%= link_to 'Back', doctors_path, class:"btn btn-success" %>
        <%= f.submit 'Create Doctor', class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
